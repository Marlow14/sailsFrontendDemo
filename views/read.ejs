<%- partial ('partials/statusmessage.ejs') %>

<h2>Family Contacts</h2>
<table class="table table-striped table-bordered">
  <tr>
    <th>ID</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Phone Number</th>
    <th>Cell Phone</th>
    <th>Email Address</th>
    <th>Home Address</th>
    <th>Birth Date</th>
    <th>Active</th>
  </tr>
  <% if(typeof families != "undefined" && families.length > 0){ %>
    <% families.forEach(function(family){ %>
      <tr>
        <td><%= family.id %></td>
        <td><%= family.firstName %></td>
        <td><%= family.lastName %></td>
        <td><%= family.phoneNumber %></td>
        <td><%= family.cellPhone %></td>
        <td><%= family.emailAddress %></td>
        <td><%= family.homeAddress %></td>
        <td><%= family.birthDate %></td>
        <td><%= family.active %></td>
      </tr>
    <% }) %>
  <% }else{ %>
      <tr>
        <td colspan="9">0 records found</td>
      </tr>
  <% } %>
  
</table>


